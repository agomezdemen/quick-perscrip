<h1>Sign Up</h1>
<%= form_for @patient do |f| %>
    <label> Patient name:</label><br>
    <%= f.text_field :name %><br>
    <label> Patient email:</label><br>
    <%= f.text_field :email %><br>
    <label> Patient password:</label><br>
    <%= f.password_field :password %><br>
    <%= f.submit %>
<%end%>
